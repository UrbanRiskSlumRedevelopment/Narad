<?xml version="1.0" encoding="utf-8"?>
<groups>
    <group name="Family">
        <gtext>Family</gtext>
        <repeatable>F</repeatable>
        <question name="Head of Family">
            <qtext>Name of Head of the Family</qtext>
            <qtype>T</qtype>
            <qhint>Enter text</qhint>
            <req>T</req>
        </question>
        <question name="Camera">
            <qtext>Camera</qtext>
            <qtype>C</qtype>
            <qhint>None</qhint>
            <req>F</req>
        </question>
        <question name="Consumer Durables">
            <qtext>Consumer Durables</qtext>
            <req>F</req>
            <qhint>Check all that are owned by family</qhint>
            <qtype>MC</qtype>
            <qchoices>
                <choice>
                    <ctext>Electric Fan</ctext>
                </choice>
                <choice>
                    <ctext>Refrigerator</ctext>
                </choice>
                <choice>
                    <ctext>Cooler</ctext>
                </choice>
                <choice>
                    <ctext>Residential Telephone</ctext>
                </choice>
                <choice>
                    <ctext>Mobile Phone</ctext>
                </choice>
            </qchoices>
        </question>
        <question name="Sex">
            <qtext>Sex</qtext>
            <qtype>SC</qtype>
            <qhint>Select one</qhint>
            <req>T</req>
            <qchoices>
                <choice>
                    <ctext>Female</ctext>
                </choice>
                <choice>
                    <ctext>Male</ctext>
                </choice>
            </qchoices>
        </question>
        <question name="Father's Name">
            <qtext>Father's Name</qtext>
            <qtype>T</qtype>
            <qhint>Enter text</qhint>
            <req>F</req>
        </question>
        <question name="Poverty Line">
            <qtext>Is your family below poverty line?</qtext>
            <qtype>SC</qtype>
            <qhint>Select one</qhint>
            <qchoices>
                <choice>
                    <ctext>Yes</ctext>
                </choice>
                <choice>
                    <ctext>No</ctext>
                </choice>
                <choice>
                    <ctext>Don't know</ctext>
                </choice>
            </qchoices>
            <req>T</req>
        </question>
        <question name="Illiterate females">
            <qtext>Number of illiterate female adult members (Age more than 14 years)</qtext>
            <qtype>N</qtype>
            <qhint>Enter number</qhint>
            <req>T</req>
        </question>
        <question name="Illiterate males">
            <qtext>Number of illiterate male adult members (Age more than 14 years)</qtext>
            <qtype>N</qtype>
            <qhint>Enter number</qhint>
            <req>T</req>
        </question>
    </group>
    <group name="Map">
        <gtext>Map</gtext>
        <repeatable>F</repeatable>
        <question name="Map">
            <qtext>Map</qtext>
            <qtype>M</qtype>
            <qhint>None</qhint>
            <req>F</req>
        </question>
    </group>
    <group name="Test (repeatable)">
        <gtext>Test</gtext>
        <repeatable>T</repeatable>
        <question name="test1">
            <qtext>Test question 1 for repeatable chunk</qtext>
            <qtype>T</qtype>
            <qhint>Enter anything</qhint>
            <req>T</req>
        </question>
        <question name="test2">
            <qtext>Text question 2</qtext>
            <qtype>T</qtype>
            <qhint>Enter anything</qhint>
            <req>F</req>
        </question>
        <rchunk>
            <rtext>TEst chunk</rtext>
            <rquestion name="test3">
                <qtype>T</qtype>
                <qtext>ID</qtext>
                <qhint>just a test</qhint>
                <req>T</req>
            </rquestion>
        </rchunk>
        <rchunk>
            <rtext>TEst chunk 2</rtext>
            <rquestion name="test4">
                <qtype>T</qtype>
                <qtext>ID</qtext>
                <qhint>just a test</qhint>
                <req>T</req>
            </rquestion>
            <rquestion name="test5">
                <qtype>T</qtype>
                <qtext>Testing 2</qtext>
                <qhint>just a test</qhint>
                <req>F</req>
            </rquestion>
        </rchunk>
    </group>
    <group name="Linked Questions">
        <gtext>Linked Questions</gtext>
        <repeatable>F</repeatable>
        <question name="Another Question">
            <qtext>Would you like another question?</qtext>
            <qtype>LC</qtype>
            <qhint>Select one</qhint>
            <qchoices>
                <choice>
                    <ctext>Yes</ctext>
                    <extra>T</extra>
                    <equestion name="Enter Whatever">
                        <qtext>Enter some text</qtext>
                        <qhint>Enter anything</qhint>
                        <qtype>T</qtype>
                    </equestion>
                    <equestion name="More Whatever">
                        <qtext>Enter any number</qtext>
                        <qhint>Enter anything</qhint>
                        <qtype>N</qtype>
                    </equestion>
                </choice>
                <choice>
                    <ctext>No</ctext>
                    <extra>F</extra>
                </choice>
                <choice>
                    <ctext>Don't know</ctext>
                    <extra>F</extra>
                </choice>
            </qchoices>
            <req>T</req>
        </question>
    </group>
</groups>