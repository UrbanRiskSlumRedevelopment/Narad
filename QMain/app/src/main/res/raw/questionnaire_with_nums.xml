<resources>
    <metadata>
        <title>Housing Survey</title>
        <idfield>City</idfield>
        <idfield>Location - Ward No/Name</idfield>
        <idfield>Name of Slum</idfield>
        <idfield>House/Flat/Door No.</idfield>
    </metadata>
    <groups>
        <group no="I" name="general_info">
            <gtext>General Information</gtext>
            <question>
                <q>1</q>
                <qtext>City</qtext>
                <qtype>T</qtype>
                <qhint>Enter city</qhint>
                <req>T</req>
                <uid>T</uid>
            </question>
            <question>
                <q>2</q>
                <qtext>Location - Ward No/Name</qtext>
                <qtype>T</qtype>
                <req>T</req>
                <qhint>Enter location</qhint>
                <uid>T</uid>
            </question>
            <question>
                <q>3</q>
                <qtext>Name of Slum</qtext>
                <qtype>T</qtype>
                <qhint>Enter slum</qhint>
                <req>T</req>
                <uid>T</uid>
            </question>
            <question>
                <q>4</q>
                <qtext>House/Flat/Door No.</qtext>
                <qtype>N</qtype>
                <qhint>Enter number</qhint>
                <req>T</req>
                <uid>T</uid>
            </question>
        </group>
        <group no="II" name="family">
            <gtext>Household Level General Information</gtext>
            <question name="Head of Family">
                <q>5</q>
                <qtext>Name of Head of the Family</qtext>
                <qtype>T</qtype>
                <qhint>Enter text</qhint>
                <req>T</req>
            </question>
            <question name="Minority Status">
                <q>6</q>
                <qtext>Minority Status</qtext>
                <qtype>SC</qtype>
                <qhint></qhint>
                <req>T</req>
                <choices>
                    <choice>
                        <ctext>Non-Minority</ctext>
                    </choice>
                    <choice>
                        <ctext>Minority</ctext>
                    </choice>
                </choices>
            </question>
            <question name="Female-headed">
                <q>7</q>
                <qtext>Female-headed Household?</qtext>
                <qtype>SC</qtype>
                <qhint>Select one</qhint>
                <choices>
                    <choice>
                        <ctext>Yes</ctext>
                        <dependents>8</dependents>
                    </choice>
                    <choice>
                        <ctext>No</ctext>
                        <extra>F</extra>
                    </choice>
                </choices>
                <req>T</req>
            </question>
            <question>
                <q>8</q>
                <inv>T</inv>
                <qtext>Status of female head?</qtext>
                <qhint>Select one</qhint>
                <qtype>SC</qtype>
                <req>F</req>
                <choices>
                    <choice>
                        <ctext>Married</ctext>
                    </choice>
                    <choice>
                        <ctext>Widowed</ctext>
                    </choice>
                    <choice>
                        <ctext>Abandoned/Single</ctext>
                    </choice>
                    <choice>
                        <ctext>Divorced</ctext>
                    </choice>
                    <choice>
                        <ctext>Unwed Mother</ctext>
                    </choice>
                    <choice>
                        <ctext>Other</ctext>
                    </choice>
                </choices>
            </question>
            <question>
                <q>9</q>
                <qtype>S</qtype>
                <qtext>Number of family members</qtext>
                <qhint>Enter number</qhint>
                <factor>
                    <ftext>Male</ftext>
                </factor>
                <factor>
                    <ftext>Female</ftext>
                </factor>
                <req>T</req>
            </question>
            <question name="Poverty Line">
                <q>10</q>
                <qtext>Is your family below the poverty line?</qtext>
                <qtype>SC</qtype>
                <qhint>Select one</qhint>
                <choices>
                    <choice>
                        <ctext>Yes</ctext>
                        <dependents>11</dependents>
                    </choice>
                    <choice>
                        <ctext>No</ctext>
                    </choice>
                    <choice>
                        <ctext>Don't know</ctext>
                    </choice>
                </choices>
                <req>T</req>
            </question>
            <question>
                <q>11</q>
                <inv>T</inv>
                <qtext>Does the family possess BPL card?</qtext>
                <qhint>Select one</qhint>
                <qtype>SC</qtype>
                <req>F</req>
                <choices>
                    <choice>
                        <ctext>Yes</ctext>
                    </choice>
                    <choice>
                        <ctext>No</ctext>
                    </choice>
                </choices>
            </question>
            <question>
                <q>12</q>
                <qtext>Household Appliances</qtext>
                <qhint>Select all that apply</qhint>
                <qtype>MC</qtype>
                <req>F</req>
                <choices>
                    <choice>
                        <ctext>Cell Phone</ctext>
                        <dependents>13</dependents>
                    </choice>
                    <choice>
                        <ctext>Refrigerator</ctext>
                    </choice>
                    <choice>
                        <ctext>Television</ctext>
                        <dependents>13</dependents>
                    </choice>
                    <choice>
                        <ctext>Washing Machine</ctext>
                    </choice>
                </choices>
            </question>
            <question>
                <q>13</q>
                <inv>T</inv>
                <qtext>Service Provider?</qtext>
                <qhint>Enter TV/phone service provider</qhint>
                <qtype>T</qtype>
                <req>F</req>
            </question>
        </group>
        <group no="III" name="earning_members">
            <gtext>Details of Earning Members of the Household</gtext>
            <rchunk>
                <q>14</q>
                <rtext>Details of Earning Members of the Household</rtext>
                <rsize>Number of earning members</rsize>
                <rlimit>20</rlimit>
                <rquestion name="test4">
                    <q>14a</q>
                    <qtype>T</qtype>
                    <qtext>Name</qtext>
                    <qhint>Enter name</qhint>
                    <req>T</req>
                </rquestion>
                <rquestion name="test5">
                    <q>14b</q>
                    <qtype>T</qtype>
                    <qtext>Relation to head of family</qtext>
                    <qhint>Enter relation</qhint>
                    <req>F</req>
                </rquestion>
                <rquestion>
                    <q>14c</q>
                    <qtype>N</qtype>
                    <qtext>Age</qtext>
                    <qhint>Enter age</qhint>
                    <req>F</req>
                </rquestion>
                <rquestion>
                    <q>14d</q>
                    <qtype>SC</qtype>
                    <qtext>Sex</qtext>
                    <qhint>Select one</qhint>
                    <req>F</req>
                    <choices>
                        <choice>
                            <ctext>Male</ctext>
                        </choice>
                        <choice>
                            <ctext>Female</ctext>
                        </choice>
                    </choices>
                </rquestion>
                <rquestion>
                    <q>15</q>
                    <qtype>P</qtype>
                    <qhint></qhint>
                    <qtext>Siblings</qtext>
                    <req>F</req>
                </rquestion>
                <rquestion>
                    <q>15a</q>
                    <qtype>N</qtype>
                    <qtext>Brothers</qtext>
                    <qhint>Enter number of brothers</qhint>
                    <req>F</req>
                    <parent>15</parent>
                </rquestion>
                <rquestion>
                    <q>15b</q>
                    <qtype>N</qtype>
                    <qtext>Sisters</qtext>
                    <qhint>Enter number of sisters</qhint>
                    <req>F</req>
                    <parent>15</parent>
                </rquestion>
            </rchunk>
        </group>
        <group no="IV" name="household_detail">
            <gtext>Household Level Detailed Information</gtext>
            <question>
                <q>24</q>
                <qtext>Source of drinking water</qtext>
                <qhint></qhint>
                <qtype>SC</qtype>
                <req>F</req>
                <choices>
                    <choice>
                        <ctext>Tap</ctext>
                    </choice>
                    <choice>
                        <ctext>Tubewell/handpump</ctext>
                    </choice>
                    <choice>
                        <ctext>Open well</ctext>
                    </choice>
                    <choice>
                        <ctext>Public tap</ctext>
                        <dependents>25</dependents>
                    </choice>
                    <choice>
                        <ctext>Tank/pond</ctext>
                        <dependents>25</dependents>
                    </choice>
                </choices>
            </question>
            <question>
                <q>25</q>
                <qtext>If outside premises, then distance from drinking water source</qtext>
                <qtype>SC</qtype>
                <qhint></qhint>
                <req>F</req>
                <inv>T</inv>
                <choices>
                    <choice>
                        <ctext>Less than 0.5 kms</ctext>
                    </choice>
                    <choice>
                        <ctext>0.5 to 1.0 km</ctext>
                    </choice>
                    <choice>
                        <ctext>1.0 to 2.0 km</ctext>
                    </choice>
                    <choice>
                        <ctext>2.0 to 5.0 km</ctext>
                    </choice>
                    <choice>
                        <ctext>More than 5.0 km</ctext>
                    </choice>
                </choices>
            </question>
            <question>
                <q>26</q>
                <qtype>SC</qtype>
                <qhint></qhint>
                <qtext>Existence of toilet facility</qtext>
                <req>F</req>
                <choices>
                    <choice>
                        <ctext>Own septic tank/flush latrine</ctext>
                    </choice>
                    <choice>
                        <ctext>Own dry latrine</ctext>
                    </choice>
                    <choice>
                        <ctext>Shared septic tank/flush latrine</ctext>
                    </choice>
                    <choice>
                        <ctext>Shared dry latrine</ctext>
                    </choice>
                    <choice>
                        <ctext>Community septic tank/flush latrine</ctext>
                        <dependents>25</dependents>
                    </choice>
                    <choice>
                        <ctext>Community dry latrine</ctext>
                        <dependents>25</dependents>
                    </choice>
                </choices>
            </question>
        </group>
        <group no="V" name="livelihood">
            <gtext>Livelihood Surveys</gtext>
            <question>
                <q>40</q>
                <qtype>P</qtype>
                <qhint>Enter primary and secondary occupations held by family members</qhint>
                <qtext>Source of earning/livelihood</qtext>
                <req>F</req>
            </question>
            <question>
                <q>40a</q>
                <qtype>P</qtype>
                <qhint></qhint>
                <qtext>Male</qtext>
                <parent>40</parent>
                <req>F</req>
            </question>
            <question>
                <q>40b</q>
                <qtype>P</qtype>
                <qhint></qhint>
                <qtext>Female</qtext>
                <req>F</req>
                <parent>40</parent>
            </question>
            <question>
                <q>40a1</q>
                <qtype>T</qtype>
                <qhint>Enter occupation</qhint>
                <qtext>Primary occupation</qtext>
                <req>F</req>
                <parent>40a</parent>
            </question>
            <question>
                <q>40a2</q>
                <qtype>T</qtype>
                <qhint>Enter occupation</qhint>
                <qtext>Secondary occupation</qtext>
                <req>F</req>
                <parent>40a</parent>
            </question>
            <question>
                <q>40b1</q>
                <qtype>T</qtype>
                <qhint>Enter occupation</qhint>
                <qtext>Primary occupation</qtext>
                <req>F</req>
                <parent>40b</parent>
            </question>
            <question>
                <q>40b2</q>
                <qtype>T</qtype>
                <qhint>Enter occupation</qhint>
                <qtext>Secondary occupation</qtext>
                <parent>40b</parent>
                <req>F</req>
            </question>
        </group>
    </groups>
</resources>
